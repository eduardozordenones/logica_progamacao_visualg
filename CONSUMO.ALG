Algoritmo "consumo"

 // atividade - consumo


Var

  distancia: inteiro
  combustivel, consumo_medio: real


Inicio

   escreval ("Distancia percorrida: ")
   leia (distancia)
   
   escreval ("Combustível gasto: ")
   leia (combustivel)
   
   consumo_medio <- distancia / combustivel
   
   escreval ("Consumo medio: ", consumo_medio:5:3)





Fimalgoritmo